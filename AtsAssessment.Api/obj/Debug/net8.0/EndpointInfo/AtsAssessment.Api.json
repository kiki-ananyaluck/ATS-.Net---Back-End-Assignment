{
  "openapi": "3.0.1",
  "info": {
    "title": "ATS API",
    "version": "v1"
  },
  "paths": {
    "/api/Auth/token": {
      "post": {
        "tags": [
          "Auth"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/JwtRequest"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/JwtRequest"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/JwtRequest"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/api/Employee/with-department": {
      "get": {
        "tags": [
          "Employee"
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/api/Rank": {
      "post": {
        "tags": [
          "Rank"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/RankRequest"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/RankRequest"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/RankRequest"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/api/WeatherProxy": {
      "get": {
        "tags": [
          "WeatherProxy"
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "JwtRequest": {
        "type": "object",
        "properties": {
          "username": {
            "type": "string",
            "nullable": true
          },
          "password": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "RankRequest": {
        "type": "object",
        "properties": {
          "p1": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      }
    },
    "securitySchemes": {
      "Bearer": {
        "type": "http",
        "description": "Enter your JWT token below (Bearer + token not required)",
        "scheme": "Bearer",
        "bearerFormat": "JWT"
      }
    }
  },
  "security": [
    {
      "Bearer": [ ]
    }
  ]
}